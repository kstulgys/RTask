{
  "name": "react-ts",
  "version": "1.0.0",
  "description": "React and TypeScript example starter project",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@chakra-ui/core": "0.6.1",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@reduxjs/toolkit": "^1.3.2",
    "axios": "0.19.2",
    "date-fns": "^2.11.0",
    "debounce": "^1.2.0",
    "emotion-theming": "10.0.27",
    "framer-motion": "^1.9.1",
    "json-server": "^0.16.1",
    "lowdb": "^1.0.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-redux": "^7.2.0",
    "react-scripts": "^3.4.0",
    "react-vis": "^1.11.7",
    "react-vis-types": "^0.0.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@jackfranklin/test-data-bot": "^1.0.0",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/debounce": "^1.2.0",
    "@types/jest": "^25.1.4",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^13.9.1",
    "@types/numeral": "^0.0.26",
    "@types/ramda": "^0.26.44",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/recharts": "^1.8.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/testing-library__cypress": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "coveralls": "^3.0.11",
    "cypress": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "faker": "^4.1.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "ramda": "^0.27.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "dev": "netlify dev",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test:coveralls": "react-scripts test --env=jsdom --coverage && coveralls < coverage/lcov.info",
    "cypress": "cypress open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!<rootDir>/node_modules/**/*",
      "!<rootDir>/src/test/**/*",
      "!<rootDir>/src/dev-tools/**/*"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
